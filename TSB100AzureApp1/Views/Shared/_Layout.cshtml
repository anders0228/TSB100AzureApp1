<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container-fluid">
        <div class="row">
            <div class="col-12">
                <div class="header row">
                    <div class="col-md-9 col-sm-12">
                        <img alt="Finder" src="//i.imgur.com/YHlSO2T.png" style="max-width: 100% ;
         min-height: 53px ; cursor:pointer" onclick="window.location = '/';" />
                    </div>
                </div>
            </div>
            <div class="col-12">
                <div class="line"> </div>
                <div class="header-bottom">
                    @Html.ActionLink("Hem", "Index", "Home")
                    @Html.ActionLink("Om", "About", "Home")
                </div>
                <div class="line"> </div>
            </div>
        </div>
    </div>
    <div class="container body-content">
        <br />
        @RenderBody()
        <hr />
        <footer class="footerMargin">
            <p>@DateTime.Now.Year - UserProfileStatistics</p>
        </footer>
    </div>

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
